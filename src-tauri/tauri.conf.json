{
  "build": {
    "beforeDevCommand": "yarn dev",
    "devPath": "http://localhost:5173/",
    "beforeBuildCommand": "yarn build",
    "distDir": "../dist"
  },
  "package": {
    "productName": "remove-time",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "dialog": {
        "all": true
      },
      "fs": {
        "all": true,
        "readDir": true,
        "readFile": true,
        "writeFile": true
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.your-app.dev",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null,
        "minimumSystemVersion": "10.13"
      },
      "resources": [],
      "shortDescription": "",
      "targets": [
        "msi"
      ],
      "windows": {
        "webviewInstallMode": {
          "type": "offlineInstaller"
        },
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 400,
        "resizable": false,
        "title": "remove time",
        "width": 500
      }
    ]
  }
}